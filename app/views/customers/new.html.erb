 <h1>New Customers</h1>

   <h1>FORM FOR</h1>
   <%= form_for @customer do |f|%>
   <br>
     <%= f.label :name %>
     <%= f.text_field :name %>
     <br>
     <%= f.label :dob %>
     <%= f.text_field :dob %>
     <br>
     <%= f.label :bio %>
     <%= f.text_field :bio %>
     <br>
     <%= f.label :user_name %>
     <%= f.text_field :user_name %>
     <br>
     <%= f.label :pasword %>
     <%= f.text_field :pasword%>
     <br>
     <%= f.label :email %>
     <%= f.text_field :email %>
     <br>
     <%= f.submit "Create Customer!" %>
     <% end %>

